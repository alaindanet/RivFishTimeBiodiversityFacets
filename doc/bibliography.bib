	pages = {1737--1751},
	file = {Chase et al. - 2018 - Embracing scale‐dependence to achieve a deeper und.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/SNBDCFLU/Chase et al. - 2018 - Embracing scale‐dependence to achieve a deeper und.pdf:application/pdf},
