@article{loreau_biodiversity_1998,
	title = {Biodiversity and ecosystem functioning: {A} mechanistic model},
	volume = {95},
	copyright = {Copyright © 1998, The National Academy of Sciences},
	issn = {0027-8424, 1091-6490},
	shorttitle = {Biodiversity and ecosystem functioning},
