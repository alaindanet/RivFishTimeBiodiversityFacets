	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13608},
	keywords = {light, temperature, climate change, secondary production, metabolic theory, seasonality},
	pages = {1809--1819},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/RY79X8NW/Junker et al. - 2020 - Resource supply governs the apparent temperature d.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/T5IIMT92/ele.html:text/html},
