	keywords = {evenness, richness, complementarity effect, selection effect, facilitation, long-term data, niche partitioning, Biodiversity–ecosystem functioning, ecosystem conservation, net biodiversity effect},
	pages = {443--451},
	file = {Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/YJQWL973/j.1461-0248.2009.01299.html:text/html;Version soumise:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/WM624J97/Isbell et al. - 2009 - Biodiversity, productivity and the temporal stabil.pdf:application/pdf},
