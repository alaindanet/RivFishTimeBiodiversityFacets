	keywords = {Dynamics, Embodied energy, Energy analysis, Energy cost of living, Energy intensity, Energy tax, Indirect effects, Input–output analysis, Path length, Residence time, Steady state, Trophic ecology, Trophic level/position},
	pages = {1072--1083},
