	volume = {158},
	issn = {0003-0147},
	url = {https://www.journals.uchicago.edu/doi/abs/10.1086/321317},
	doi = {10.1086/321317},
	abstract = {Diversity (or biodiversity) is typically measured by a species count (richness) and sometimes with an evenness index; it may also be measured by a proportional statistic that combines both measures (e.g., Shannon‐Weiner index or H′H′H{\textasciicircum}\{{\textbackslash}prime \}). These diversity measures are hypothesized to be positively and strongly correlated, but this null hypothesis has not been tested empirically. We used the results of Caswell's neutral model to generate null relationships between richness (S), evenness (J′J′J{\textasciicircum}\{{\textbackslash}prime \}), and proportional diversity (H′H′H{\textasciicircum}\{{\textbackslash}prime \}). We tested predictions of the null model against empirical relationships describing data in a literature survey and in four individual studies conducted across various scales. Empirical relationships between logSlogS{\textbackslash}mathrm\{log\}{\textbackslash},S or J′J′J{\textasciicircum}\{{\textbackslash}prime \} and H′H′H{\textasciicircum}\{{\textbackslash}prime \} differed from the null model when {\textless}10 species were tested and in plants, vertebrates, and fungi. The empirical relationships were similar to the null model when {\textgreater}10 and {\textless}100 species were tested and in invertebrates. If {\textgreater}100 species were used to estimate diversity, the relation between logSlogS{\textbackslash}mathrm\{log\}{\textbackslash},S and H′H′H{\textasciicircum}\{{\textbackslash}prime \} was negative. The strongest predictive models included logSlogS{\textbackslash}mathrm\{log\}{\textbackslash},S and J′J′J{\textasciicircum}\{{\textbackslash}prime \}. A path analysis indicated that logSlogS{\textbackslash}mathrm\{log\}{\textbackslash},S and J′J′J{\textasciicircum}\{{\textbackslash}prime \} were always negatively related, that empirical observations could not be explained without including indirect effects, and that differences between the partials may indicate ecological effects, which suggests that S and J′J′J{\textasciicircum}\{{\textbackslash}prime \} act like diversity components or that diversity should be measured using a compound statistic.},
	number = {3},
	urldate = {2021-02-05},
	journal = {The American Naturalist},
	author = {Stirling, Gray and Wilsey, Brian},
	month = sep,
	year = {2001},
	note = {Publisher: The University of Chicago Press},
	pages = {286--299},
	file = {Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/8IVD6BR8/321317.html:text/html;Texte intégral:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/KAZ2BXY7/Stirling et Wilsey - 2001 - Empirical Relationships between Species Richness, .pdf:application/pdf},
}

@article{blois_space_2013,
	title = {Space can substitute for time in predicting climate-change effects on biodiversity},
	volume = {110},
	copyright = {©  . Freely available online through the PNAS open access option.},
	issn = {0027-8424, 1091-6490},
	url = {https://www.pnas.org/content/110/23/9374},
	doi = {10.1073/pnas.1220228110},
	abstract = {“Space-for-time” substitution is widely used in biodiversity modeling to infer past or future trajectories of ecological systems from contemporary spatial patterns. However, the foundational assumption—that drivers of spatial gradients of species composition also drive temporal changes in diversity—rarely is tested. Here, we empirically test the space-for-time assumption by constructing orthogonal datasets of compositional turnover of plant taxa and climatic dissimilarity through time and across space from Late Quaternary pollen records in eastern North America, then modeling climate-driven compositional turnover. Predictions relying on space-for-time substitution were ∼72\% as accurate as “time-for-time” predictions. However, space-for-time substitution performed poorly during the Holocene when temporal variation in climate was small relative to spatial variation and required subsampling to match the extent of spatial and temporal climatic gradients. Despite this caution, our results generally support the judicious use of space-for-time substitution in modeling community responses to climate change.},
	language = {en},
	number = {23},
	urldate = {2021-02-05},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Blois, Jessica L. and Williams, John W. and Fitzpatrick, Matthew C. and Jackson, Stephen T. and Ferrier, Simon},
	month = jun,
	year = {2013},
	pmid = {23690569},
	note = {Publisher: National Academy of Sciences
Section: Biological Sciences},
	keywords = {global change, fossil pollen, generalized dissimilarity modeling, paleoecology},
	pages = {9374--9379},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/LCFTKR83/Blois et al. - 2013 - Space can substitute for time in predicting climat.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/TJTSLKYR/9374.html:text/html},
