@article{bahlai_broken_2021,
	title = {The broken window: {An} algorithm for quantifying and characterizing misleading trajectories in ecological processes},
	volume = {64},
	issn = {15749541},
	shorttitle = {The broken window},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1574954121001278},
	doi = {10.1016/j.ecoinf.2021.101336},
	abstract = {A core issue in temporal ecology is the concept of trajectory—that is, when can ecologists have reasonable assurance that they know where a system is going? In this paper, we describe a non-random resampling method to directly address the temporal aspects of scaling ecological observations by leveraging existing data. Findings from long-term research sites have been hugely influential in ecology because of their unprecedented longitu­ dinal perspective, yet short-term studies more consistent with typical grant cycles and graduate programs are still the norm. We use long-term insights to create ‘broken windows,’ that is, reanalyze long-term studies from shortterm observational perspectives to examine discontinuities in trends at differing temporal scales.},
	language = {en},
	urldate = {2021-09-21},
	journal = {Ecological Informatics},
	author = {Bahlai, Christie A. and White, Easton R. and Perrone, Julia D. and Cusser, Sarah and Stack Whitney, Kaitlin},
	month = sep,
	year = {2021},
	pages = {101336},
	file = {Bahlai et al. - 2021 - The broken window An algorithm for quantifying an.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/TY46BYTT/Bahlai et al. - 2021 - The broken window An algorithm for quantifying an.pdf:application/pdf},
}

