@article{mcintire_eliot_j._b._facilitation_2013,
	title = {Facilitation as a ubiquitous driver of biodiversity},
	volume = {201},
	issn = {0028-646X},
	url = {https://nph.onlinelibrary.wiley.com/doi/full/10.1111/nph.12478},
	doi = {10.1111/nph.12478},
	abstract = {403 I. 403 II. 405 III. 410 IV. 411 V. 412 VI. 412 VII. 413 VIII. 413 413 References 414 Summary Models describing the biotic drivers that create and maintain biological diversity within trophic levels have focused primarily on negative interactions (i.e. competition), leaving marginal room for positive interactions (i.e. facilitation). We show facilitation to be a ubiquitous driver of biodiversity by first noting that all species use resources and thus change the local biotic or abiotic conditions, altering the available multidimensional niches. This can cause a shift in local species composition, which can cause an increase in beta, and sometimes alpha, diversity. We show that these increases are ubiquitous across ecosystems. These positive effects on diversity occur via a broad host of disparate direct and indirect mechanisms. We identify and unify several of these facilitative mechanisms and discuss why it has been easy to underappreciate the importance of facilitation. We show that net positive effects have a long history of being considered ecologically or evolutionarily unstable, and we present recent evidence of its potential stability. Facilitation goes well beyond the common case of stress amelioration and it probably gains importance as community complexity increases. While biodiversity is, in part, created by species exploiting many niches, many niches are available to exploit only because species create them.},
	number = {2},
	journal = {New Phytologist},
	author = {{McIntire Eliot J. B.} and {Fajardo Alex}},
	month = sep,
	year = {2013},
	keywords = {Facilitation, positive interactions, coexistence, Biodiversity, intransitive competition, plant functional traits, resource sharing, stress gradient hypothesis (SGH)},
	pages = {403--416},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/KKVSVZ8K/McIntire Eliot J. B. et Fajardo Alex - 2013 - Facilitation as a ubiquitous driver of biodiversit.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/DDHU47BM/nph.html:text/html},
}

@article{farrior_dominance_2016,
	title = {Dominance of the suppressed: {Power}-law size structure in tropical forests},
	volume = {351},
