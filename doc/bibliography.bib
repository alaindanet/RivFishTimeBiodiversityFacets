	year = {2016},
	keywords = {herbivory, aboveground-belowground interactions, allocation, chemical defenses, growth-defense tradeoff, Neotropical savanna, plant-herbivore interactions, plant responses, Solanum lycocarpum},
	pages = {n/a--n/a},
