@article{thibaut_understanding_2013,
	title = {Understanding diversity–stability relationships: towards a unified model of portfolio effects},
	volume = {16},
	copyright = {© 2012 Blackwell Publishing Ltd/CNRS},
	issn = {1461-0248},
	shorttitle = {Understanding diversity–stability relationships},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.12019},
	doi = {10.1111/ele.12019},
	abstract = {A major ecosystem effect of biodiversity is to stabilise assemblages that perform particular functions. However, diversity–stability relationships (DSRs) are analysed using a variety of different population and community properties, most of which are adopted from theory that makes several restrictive assumptions that are unlikely to be reflected in nature. Here, we construct a simple synthesis and generalisation of previous theory for the DSR. We show that community stability is a product of two quantities: the synchrony of population fluctuations, and an average species-level population stability that is weighted by relative abundance. Weighted average population stability can be decomposed to consider effects of the mean-variance scaling of abundance, changes in mean abundance with diversity and differences in species' mean abundance in monoculture. Our framework makes explicit how unevenness in the abundances of species in real communities influences the DSR, which occurs both through effects on community synchrony, and effects on weighted average population variability. This theory provides a more robust framework for analysing the results of empirical studies of the DSR, and facilitates the integration of findings from real and model communities.},
	language = {en},
	number = {2},
	urldate = {2019-07-15},
	journal = {Ecology Letters},
	author = {Thibaut, Loïc M. and Connolly, Sean R.},
	year = {2013},
	keywords = {evenness, insurance hypothesis, overyielding, Diversity–stability relationship, mean-variance scaling, population stability, population synchrony, portfolio effect, statistical averaging},
	pages = {140--150},
	file = {ele12019-sup-0001-appendixs1.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/ZZ4YLARA/ele12019-sup-0001-appendixs1.pdf:application/pdf;ele12019-sup-0002-appendixs2.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/TZST92V6/ele12019-sup-0002-appendixs2.pdf:application/pdf;ele12019-sup-0003-appendixs3.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/BVKPKHBL/ele12019-sup-0003-appendixs3.pdf:application/pdf;ele12019-sup-0004-appendixs4.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/RH66WGV6/ele12019-sup-0004-appendixs4.pdf:application/pdf;ele12019-sup-0005-appendixs5.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/CYWIB9HP/ele12019-sup-0005-appendixs5.pdf:application/pdf;Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/PRM3RZFE/Thibaut et Connolly - 2013 - Understanding diversity–stability relationships t.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/KDX9PWZF/ele.html:text/html},
