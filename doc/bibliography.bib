	file = {Larras et al. - 2017 - Assessing anthropogenic pressures on streams A ra.pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/N5DQCWZY/Larras et al. - 2017 - Assessing anthropogenic pressures on streams A ra.pdf:application/pdf},
}

