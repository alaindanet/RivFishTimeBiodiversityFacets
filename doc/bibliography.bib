	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13141},
	keywords = {macroecology, extinction, Biotic exchanges, functional divergence, functional richness, introduction},
	pages = {1649--1659},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/TUW8DRKB/Toussaint et al. - 2018 - Non-native species led to marked shifts in functio.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/7L4ZTWLW/ele.html:text/html},
}

@article{magurran_rapid_2015,
