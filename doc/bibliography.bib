	urldate = {2018-09-28},
	journal = {Land Degradation \& Development},
	author = {Tarhouni, Mohamed and Hmida, Walid Ben and Neffati, Mohamed},
	month = may,
	year = {2017},
	keywords = {dry area, life forms, long protection, plants, rainfall variation, Tunisia},
	pages = {1199--1211},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/FRXRX55A/Tarhouni et al. - 2017 - Long-Term Changes in Plant Life Forms as a Consequ.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/UZ2UZGY3/ldr.html:text/html},
}

