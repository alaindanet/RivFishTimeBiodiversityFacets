	month = oct,
	year = {2017},
	keywords = {Species diversity, Grasslands, Biomass (ecology), Conservation science, Herbs, Insects, Land use, Seasons},
	pages = {e0185809},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/T7J2EA7W/Hallmann et al. - 2017 - More than 75 percent decline over 27 years in tota.pdf:application/pdf},
