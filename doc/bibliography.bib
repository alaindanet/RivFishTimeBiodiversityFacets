@article{claessen_impact_2002,
	title = {The {Impact} of {Size}-{Dependent} {Predation} on {Population} {Dynamics} and {Individual} {Life} {History}},
	volume = {83},
	copyright = {© 2002 by the Ecological Society of America},
	issn = {1939-9170},
	url = {https://esajournals.onlinelibrary.wiley.com/doi/abs/10.1890/0012-9658%282002%29083%5B1660%3ATIOSDP%5D2.0.CO%3B2},
	doi = {10.1890/0012-9658(2002)083[1660:TIOSDP]2.0.CO;2},
	abstract = {In size-structured predator–prey systems, capture success depends on the sizes of both predator and prey. We study the population-dynamic consequences of size-dependent predation using a model of a size-structured, cannibalistic fish population with one shared, alternative resource. We assume that a prey can be captured by a predator if the ratio of prey length to predator length is within a specific range, referred to as the “predation window.” We find that the lower limit of the predation window (δ) has a major impact on population dynamics, whereas the upper limit (ε) mainly affects population structure and individual life history. For large δ, cannibalism cannot decimate young-of-year (YOY) cohorts. Size-dependent competition then results in recruit-driven, single-cohort cycles. With low δ, cannibalism regulates recruitment, resulting in coexistence of many year classes. With intermediate δ, periods of regulation by cannibalism alternate with periods with severe competition. Occasional high densities of small individuals enable a few cannibals to reach giant sizes, producing a bimodal population size distribution. With small ε, all individuals remain small; the population is stunted. Large piscivores can exist only if induced dynamically in population fluctuations. Above a critical ε, large piscivores are present permanently, even in stable populations. The critical effect of ε relates to the ontogenetic niche shift from planktivory to piscivory. Observed population dynamics of Eurasian perch, yellow perch, and Arctic char, described in the literature, are discussed and, based on our modeling results, can be related to differences in the predation windows of these species. We argue that the effects of δ and ε relate to two fundamentally different and mutually exclusive aspects of cannibalism.},
	language = {en},
	number = {6},
	urldate = {2020-07-15},
	journal = {Ecology},
	author = {Claessen, David and Oss, Catelijne Van and Roos, André M. de and Persson, Lennart},
	year = {2002},
	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1890/0012-9658\%282002\%29083\%5B1660\%3ATIOSDP\%5D2.0.CO\%3B2},
	keywords = {competition, life history, population dynamics, Arctic char, Eurasian perch, individual, physiologically structured population model, piscivory, predation, predator size relative to prey size, size dependent, size-dependent cannibalism, yellow perch},
	pages = {1660--1675},
	file = {Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/Y9I479TP/0012-9658(2002)083[1660TIOSDP]2.0.html:text/html;Version soumise:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/HZYZHIXM/Claessen et al. - 2002 - The Impact of Size-Dependent Predation on Populati.pdf:application/pdf},
}

