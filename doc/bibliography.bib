	author = {Cameron, Erin K. and Sundqvist, Maja K. and Keith, Sally A. and CaraDonna, Paul J. and Mousing, Erik A. and Nilsson, Karin A. and Metcalfe, Daniel B. and Classen, Aimée T.},
	year = {2019},
	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1002/ecs2.2645},
	keywords = {climate change, food webs, species interactions, freshwater, marine, extreme events, aquatic, data gaps, global, precipitation, terrestrial, warming},
	pages = {e02645},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/SJ2VRXK9/Cameron et al. - 2019 - Uneven global distribution of food web studies und.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/DJNN4QEI/ecs2.html:text/html},
