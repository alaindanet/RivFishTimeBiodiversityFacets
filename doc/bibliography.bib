	pages = {1323--1330},
	file = {Contrasting effects of diversity on the temporal stability of plant populations:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/R86UBGS4/ruijven2007.pdf:application/pdf;Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/RUB6TH7C/Ruijven et Berendse - 2007 - Contrasting effects of diversity on the temporal s.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/MY7XQF45/j.0030-1299.2007.16005.html:text/html},
