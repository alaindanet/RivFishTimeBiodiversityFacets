@article{raevel_cushion-plant_2018,
	series = {Special issue on {Alpine} and arctic plant communities : a worldwide perspective},
	title = {Cushion-plant protection determines guild-dependent plant strategies in high-elevation peatlands of the {Cordillera} {Real}, {Bolivian} {Andes}},
