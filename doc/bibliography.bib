	pages = {85--93},
	file = {Removing Allometric Effects of Body Size in Morphological Analysis:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/C3YZ3GW9/lleonart2000.pdf:application/pdf;ScienceDirect Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/PY55CX6F/Lleonart et al. - 2000 - Removing Allometric Effects of Body Size in Morpho.pdf:application/pdf;ScienceDirect Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/RQD8HGV3/S0022519300920435.html:text/html},
