	pages = {1177--1180},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/DGQ5NPZU/Eng et al. - 2019 - A neonicotinoid insecticide reduces fueling and de.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/4G5N4LQT/1177.html:text/html},
