	journal = {Science},
	author = {Gross, Thilo and Rudolf, Lars and Levin, Simon A. and Dieckmann, Ulf},
	month = aug,
	year = {2009},
	pmid = {19661430},
	note = {Publisher: American Association for the Advancement of Science
Section: Report},
	pages = {747--750},
	file = {Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/R5XYH6YT/747.html:text/html;Version soumise:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/67FXWG8K/Gross et al. - 2009 - Generalized Models Reveal Stabilizing Factors in F.pdf:application/pdf},
