@article{barbier_pyramids_2018,
	title = {Pyramids and cascades: a synthesis of food chain functioning and stability},
	copyright = {© 2018, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NonCommercial 4.0 International), CC BY-NC 4.0, as described at http://creativecommons.org/licenses/by-nc/4.0/},
	shorttitle = {Pyramids and cascades},
	url = {https://www.biorxiv.org/content/early/2018/07/03/361246},
	doi = {10.1101/361246},
	abstract = {Food chain theory is one of the cornerstones of ecology, providing many of its basic predictions, such as bottom-up and top-down control, biomass pyramids, trophic cascades and predator-prey oscillations. Yet, ninety years into this theory, the precise conditions under which these patterns may occur and persist in Nature remain subject to debate. Rather than address each pattern in isolation, we propose that they must be understood together, calling for the synthesis of a fragmented set of theories and empirical results. To do this, we propose a minimal theory that combines the energetic and dynamical approaches to food chains. We chart its predictions on a concise map, where two main regimes emerge: one regime characterized by pyramidal patterns, the other by cascade patterns, across a diversity of functioning and stability metrics. The axes of this map are governed by essential physiological and ecological variables, such as metabolic rates and self-regulation. A quantitative comparison with data sheds light on conflicting theoretical predictions and empirical puzzles, from inverted size spectra to causes of trophic cascade strength. We conclude that drawing systematic connections between stability and functioning properties is a necessary step toward testing food chain theory as a reliable description of real ecosystems.},
	language = {en},
	urldate = {2018-09-21},
	journal = {bioRxiv},
	author = {Barbier, Matthieu and Loreau, Michel},
	month = jul,
	year = {2018},
	pages = {361246},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/2LHQEVSF/Barbier et Loreau - 2018 - Pyramids and cascades a synthesis of food chain f.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/YUVI6HRK/361246.html:text/html},
}

