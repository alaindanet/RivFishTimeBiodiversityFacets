@article{resetarits_emlyn_j._testing_2017,
	title = {Testing the keystone community concept: effects of landscape, patch removal, and environment on metacommunity structure},
	volume = {99},
	issn = {0012-9658},
	shorttitle = {Testing the keystone community concept},
	url = {https://esajournals.onlinelibrary.wiley.com/doi/full/10.1002/ecy.2041},
	doi = {10.1002/ecy.2041},
	abstract = {Abstract Although the influence of regional processes on local patches is well studied, the influence of local patches and their spatial arrangement on regional processes is likely to be complex. One interesting idea is the keystone community concept (KCC); this posits that there may be some patches that have a disproportionately large effect on the metacommunity compared to other patches. We experimentally test the KCC by using replicate protist microcosm metacommunities with single?patch removals. Removing single patches had no effect on average community richness, evenness and biomass of our metacommunities, but did cause metacommunities to be assembled significantly less by local environmental conditions and more by spatial effects related to stochastic factors. Overall our results show that local patch removal can have large regional effects on structural processes, but indicate that more experiments are needed to find evidence of keystone communities.},
	number = {1},
	journal = {Ecology},
	author = {{Resetarits Emlyn J.} and {Cathey Sara E.} and {Leibold Mathew A.}},
	month = oct,
