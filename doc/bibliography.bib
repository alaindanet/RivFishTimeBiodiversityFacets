@article{devaux_selection_2010,
	title = {Selection on {Variance} in {Flowering} {Time} {Within} and {Among} {Individuals}},
	volume = {64},
	issn = {1558-5646},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1558-5646.2009.00895.x/abstract},
	doi = {10.1111/j.1558-5646.2009.00895.x},
	abstract = {We model the impact of pollinator visitation rate and behavior on the short-term evolution of population flowering phenologies determined by the distributions of flowering times within and among individual plants. Evolution of population flowering phenologies depends on the phenotypic variances and heritabilities of the within-individual mean and variance of flowering time. In the ecological scenarios we investigate selection does not produce a correlation of the mean and variance of individual flowering time. Self-incompatibility causes weak stabilizing selection on flowering time that acts to reduce the within-individual variance in flowering time. Disruptive selection due to pollinator limitation acts mostly to increase the among-individual variance in flowering time. Stabilizing selection due to pollinator attraction, or short reproductive season, acts mostly to decrease the within-individual variance in flowering time. Temporal autocorrelation of environmental stochasticity in pollinator visitation rate strongly selects to increase the within-individual variance in flowering time. These predictions can be tested by measuring the causal factors described above, partitioning the variance in population phenology within and among individuals, and estimating the inheritance of, and selection on, within-individual mean and variance of flowering time.},
	language = {en},
	number = {5},
	urldate = {2017-10-10},
	journal = {Evolution},
	author = {Devaux, Céline and Lande, Russell},
	month = may,
	year = {2010},
	keywords = {Density-dependent pollination, environmental stochasticity, heritability, inbreeding depression, quantitative genetic model, self-incompatibility},
	pages = {1311--1320},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/THZQDKKA/Devaux et Lande - 2010 - Selection on Variance in Flowering Time Within and.pdf:application/pdf;Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/3CR9P93S/Devaux et Lande - 2010 - Selection on Variance in Flowering Time Within and.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/ZVJ7JTFS/abstract.html:text/html;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/XXVXV279/abstract.html:text/html},
