	keywords = {stability, coefficient of variation, asymptotic resilience, community matrix, initial resilience, intrinsic stochastic variability, mesocosm, multivariate autoregressive models, plankton, weak interactions},
	pages = {1468--1478},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/Z3URXF6D/Downing et al. - Temporal stability vs. community matrix measures o.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/KYX8GC8R/ele.html:text/html;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/A6DMDVU5/ele.html:text/html;Temporal stability vs. community matrix measures of stability and the role of weak interactions:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/U9GI2ITZ/10.1111@ele.13538.pdf:application/pdf},
