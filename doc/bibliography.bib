@article{froese_cube_2006,
	title = {Cube law, condition factor and weight–length relationships: history, meta-analysis and recommendations},
	volume = {22},
	issn = {1439-0426},
	shorttitle = {Cube law, condition factor and weight–length relationships},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1439-0426.2006.00805.x},
	doi = {10.1111/j.1439-0426.2006.00805.x},
	abstract = {This study presents a historical review, a meta-analysis, and recommendations for users about weight–length relationships, condition factors and relative weight equations. The historical review traces the developments of the respective concepts. The meta-analysis explores 3929 weight–length relationships of the type W = aLb for 1773 species of fishes. It shows that 82\% of the variance in a plot of log a over b can be explained by allometric versus isometric growth patterns and by different body shapes of the respective species. Across species median b = 3.03 is significantly larger than 3.0, thus indicating a tendency towards slightly positive-allometric growth (increase in relative body thickness or plumpness) in most fishes. The expected range of 2.5 {\textless} b {\textless} 3.5 is confirmed. Mean estimates of b outside this range are often based on only one or two weight–length relationships per species. However, true cases of strong allometric growth do exist and three examples are given. Within species, a plot of log a vs b can be used to detect outliers in weight–length relationships. An equation to calculate mean condition factors from weight–length relationships is given as Kmean = 100aLb−3. Relative weight Wrm = 100W/(amLbm) can be used for comparing the condition of individuals across populations, where am is the geometric mean of a and bm is the mean of b across all available weight–length relationships for a given species. Twelve recommendations for proper use and presentation of weight–length relationships, condition factors and relative weight are given.},
	language = {en},
	number = {4},
	urldate = {2020-05-12},
	journal = {Journal of Applied Ichthyology},
	author = {Froese, R.},
	year = {2006},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1439-0426.2006.00805.x},
	pages = {241--253},
	file = {Cube law, condition factor and weight–length relationships\: history, meta-analysis and recommendations:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/ARVD9CBQ/froese2006.pdf:application/pdf;Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/43PB34WQ/Froese - 2006 - Cube law, condition factor and weight–length relat.pdf:application/pdf;Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/59JCHTV9/Froese - 2006 - Cube law, condition factor and weight–length relat.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/WU6A579A/j.1439-0426.2006.00805.html:text/html;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/75BZ6XS8/j.1439-0426.2006.00805.html:text/html;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/CRA6KQMK/j.1439-0426.2006.00805.html:text/html},
