	pages = {20172210},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/AIHMSMP9/Johnson et al. - 2017 - Abiotic niche partitioning and negative density de.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/SCDEZAZ9/20172210.html:text/html},
}

@article{karssenberg_catastrophic_2017,
	title = {Catastrophic {Shifts} in {Semiarid} {Vegetation}-{Soil} {Systems} {May} {Unfold} {Rapidly} or {Slowly}},
	volume = {190},
	issn = {0003-0147},
	url = {http://www.journals.uchicago.edu/doi/full/10.1086/694413},
	doi = {10.1086/694413},
	abstract = {Under gradual change of a driver, complex systems may switch between contrasting stable states. For many ecosystems it is unknown how rapidly such a critical transition unfolds. Here we explore the rate of change during the degradation of a semiarid ecosystem with a model coupling the vegetation and geomorphological system. Two stable states—vegetated and bare—are identified, and it is shown that the change between these states is a critical transition. Surprisingly, the critical transition between the vegetated and bare state can unfold either rapidly over a few years or gradually over decennia up to millennia, depending on parameter values. An important condition for the phenomenon is the linkage between slow and fast ecosystems components. Our results show that, next to climate change and disturbance rates, the geological and geomorphological setting of a semiarid ecosystem is crucial in predicting its fate.},
	number = {6},
	journal = {The American Naturalist},
	author = {Karssenberg, Derek and Bierkens, Marc F. P. and Rietkerk, Max},
	month = oct,
	year = {2017},
	pages = {E145--E155},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/BZH9JV6T/Karssenberg et al. - 2017 - Catastrophic Shifts in Semiarid Vegetation-Soil Sy.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/NX5RAZBA/694413.html:text/html},
}

