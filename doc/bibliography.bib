	doi = {10.1016/B978-0-12-398315-2.00002-8},
	keywords = {Community ecology, Allometry, Arctic ecology, Climate change, Ecosystems ecology, Food webs, Global warming, Stream ecology},
	pages = {81--176},
	file = {ScienceDirect Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/2QKX7DQF/O'Gorman et al. - 2012 - Chapter 2 - Impacts of Warming on the Structure an.pdf:application/pdf;ScienceDirect Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/I7HKE48X/B9780123983152000028.html:text/html},
}

@article{ogorman_multiple_2012,
	title = {Multiple anthropogenic stressors and the structural properties of food webs},
	volume = {93},
	copyright = {© 2012 by the Ecological Society of America},
	issn = {1939-9170},
	url = {https://esajournals.onlinelibrary.wiley.com/doi/abs/10.1890/11-0982.1},
	doi = {10.1890/11-0982.1},
	abstract = {Coastal environments are among the most productive on the planet, providing a wide range of ecosystem services. Development and exploitation mean that they are faced with stresses from a number of anthropogenic sources. Such stresses are typically studied in isolation, but multiple stressors can combine in unexpected ways to alter the structure of ecological systems. Here, we experimentally explore the impacts of inorganic nutrients and organic matter on a range of food web properties. We find that these two stressors combine additively to produce significant increases in connectance and mean food chain length. Such increases are typically associated with enhanced robustness to secondary extinctions and productivity, respectively. Despite these apparent beneficial effects, we find a simplification of web structure in terms of taxon richness and diversity, and altered proportions of basal and top species. These effects are driven by a reduction in community assembly and lower consistency in a range of system properties as a result of the multiple stressors. Consequently, impacted food webs are likely to be more vulnerable to human- or climate-induced perturbations in the long term.},
	language = {en},
	number = {3},
	urldate = {2020-05-29},
	journal = {Ecology},
	author = {O'Gorman, Eoin J. and Fitch, Jayne E. and Crowe, Tasman P.},
	year = {2012},
	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1890/11-0982.1},
	keywords = {ecosystem functioning, biodiversity, marine, nitrogen deposition, nutrient enrichment, predator–prey interactions},
	pages = {441--448},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/DXKFPUUN/O'Gorman et al. - 2012 - Multiple anthropogenic stressors and the structura.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/X8MR4EPK/11-0982.html:text/html},
}

@article{duffy_ecosystem_2005,
	title = {Ecosystem consequences of diversity depend on food chain length in estuarine vegetation},
	volume = {8},
	issn = {1461-0248},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1461-0248.2005.00725.x},
	doi = {10.1111/j.1461-0248.2005.00725.x},
	abstract = {Biodiversity and food chain length each can strongly influence ecosystem functioning, yet their interactions rarely have been tested. We manipulated grazer diversity in seagrass mesocosms with and without a generalist predator and monitored community development. Changing food chain length altered biodiversity effects: higher grazer diversity enhanced secondary production, epiphyte grazing, and seagrass biomass only with predators present. Conversely, changing diversity altered top-down control: predator impacts on grazer and seagrass biomass were weaker in mixed-grazer assemblages. These interactions resulted in part from among-species trade-offs between predation resistance and competitive ability. Despite weak impact on grazer abundance at high diversity, predators nevertheless enhanced algal biomass through a behaviourally mediated trophic cascade. Moreover, predators influenced every measured variable except total plant biomass, suggesting that the latter is an insensitive metric of ecosystem functioning. Thus, biodiversity and trophic structure interactively influence ecosystem functioning, and neither factor's impact is predictable in isolation.},
	language = {en},
	number = {3},
	urldate = {2020-05-29},
	journal = {Ecology Letters},
	author = {Duffy, J. Emmett and Richardson, J. Paul and France, Kristin E.},
	year = {2005},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2005.00725.x},
	keywords = {grazing, Biodiversity, ecosystem functioning, food webs, trophic cascade, marine, response diversity, seagrass, trait-mediated indirect interactions},
	pages = {301--309},
	file = {Ecosystem consequences of diversity depend on food chain length in estuarine vegetation:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/J7JG9YS6/emmettduffy2005.pdf:application/pdf;Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/CVHRQY3W/Duffy et al. - 2005 - Ecosystem consequences of diversity depend on food.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/ZR5QL6BJ/j.1461-0248.2005.00725.html:text/html},
