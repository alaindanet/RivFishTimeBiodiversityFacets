@techreport{pri_large-scale_2020,
	type = {preprint},
	title = {Large-scale monitoring of freshwater bivalves: an {eDNA} point of view on species distribution and conservation},
	shorttitle = {Large-scale monitoring of freshwater bivalves},
	url = {https://www.authorea.com/users/317031/articles/448890-large-scale-monitoring-of-freshwater-bivalves-an-edna-point-of-view-on-species-distribution-and-conservation?commit=0de737902592bd80604ab2562834469cf25527cb},
	abstract = {The analysis of environmental DNA (eDNA) allows eﬃcient surveys of freshwater species and is being increasingly used. However, most studies generally have a limited sampling plan. Because diﬀerent methods are used by diﬀerent authors, all the data produced with eDNA cannot be easily pooled together for a global overview. Nevertheless, one of the promising perspectives is the standardization of the methods and protocols, for long-term monitoring, early detection of invasive species, and rare species detection. We here present the biggest dataset using eDNA metabarcoding with standardized methods for freshwater bivalves. Sampling was performed mainly in France, with over 350 localities spread over all major basins. Metabarcoding was performed using two new sets of primers, one for the Unionida and one for the Venerida. The species distributions inferred from eDNA sampling are then compared with those currently known. Results give an insight into the valuable data that eDNA analysis can provide if used at a large scale. Some species were found outside of their known range, especially invasive species but also species of conservation interest. Others were found in a surprisingly low number of localities, although currently considered widespread and of no conservation interest. This can be explained by the databases currently used to establish conservation status, which are often biased by including ancient data, shell-only data, and potentially misidentiﬁed species. The present study shows that our eDNA metabarcoding approach will be an essential tool to unveil the true species distributions, to better evaluate their status and improve conservation policymaking.},
	language = {en},
	urldate = {2020-09-02},
	institution = {Preprints},
	author = {Pri, Vincent and Lima, Manuel Lopes and Taberlet, Pierre and Valentini, Alice and Poulet, Nicolas and Jean, Pauline and Breugnot, Emilie and Couprie, St phanie and Jardin, Ga lle and Roset, Nicolas and Vigneron, Thibault and Lamand, Florent and Gargominy, Olivier and Rocle, Mathieu and Dejean, Tony},
	month = may,
	year = {2020},
	doi = {10.22541/au.158888143.31089837},
	file = {Pri et al. - 2020 - Large-scale monitoring of freshwater bivalves an .pdf:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/N6HEE5BF/Pri et al. - 2020 - Large-scale monitoring of freshwater bivalves an .pdf:application/pdf},
}

