	file = {PubMed Central Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/YI3H3DAF/Bascompte et Stouffer - 2009 - The assembly and disassembly of ecological network.pdf:application/pdf},
}

