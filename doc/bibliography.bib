	year = {2016},
	keywords = {turnover, biodiversity analysis, cyclic shift, long-term data, mean rank shift, r package, rank clock, synchrony, temporal analysis, variance ratio},
	pages = {1146--1151},
	file = {Full Text PDF:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/KZ3NFRWF/Hallett et al. - 2016 - codyn An r package of community dynamics metrics.pdf:application/pdf;Snapshot:/home/alain/.mozilla/firefox/sf9zmf9b.default/zotero/storage/PARI9A8E/2041-210X.html:text/html},
